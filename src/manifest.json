{
   "background": {
      "scripts": [
       "lib/Base.js",
       "lib/DOM.js",
       "lib/Async.js",
       "lib/md5.js",
       "lib/proto.js",
       "lib/utils.js",
       "lib/repository.js",
       "lib/models.js",
       "lib/background.js",
       "lib/menu.js"
      ]
   },
   "content_scripts": [ {
      "js": [
       "lib/Base.js",
       "lib/DOM.js",
       "lib/Async.js",
       "lib/Style.js",
       "lib/Signal.js",
       "lib/proto.js",
       "lib/utils.js",
       "lib/repository.js",
       "lib/extractors.js",
       "lib/userscripts.js",
       "lib/content.js"
      ],
      "css": ["styles/general.css"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "browser_action": {
     "default_icon" : "skin/fork19.png",
     "default_title": "taberareloo",
     "default_popup": "popup.html"
   },
   "options_page": "options.html",
   "sandbox": {
     "pages": [
       "sandbox.html"
     ],
     "content_security_policy": "sandbox allow-scripts; 'unsafe-eval'"
   },
   "web_accessible_resources": [
     "skin/fork64.png"
   ],
   "icons": {
     "128": "skin/fork128.png",
     "64": "skin/fork64.png",
     "32": "skin/fork32.png",
     "16": "skin/fork16.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "21.0",
   "default_locale": "en_US",
   "description": "Yet Another Tombloo on Chromium",
   "name": "Taberareloo",
   "permissions": [ "http://*/*", "https://*/*", "tabs", "bookmarks", "contextMenus", "cookies", "unlimited_storage", "notifications", "webRequest" ],
   "version": "2.0.68",
   "incognito": "split"
}
