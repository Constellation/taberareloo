{
   "background_page": "background.html",
   "content_scripts": [ {
      "js": [
       "lib/Base.js",
       "lib/DOM.js",
       "lib/Async.js",
       "lib/Style.js",
       "lib/Signal.js",
       "lib/proto.js",
       "lib/utils.js",
       "lib/repository.js",
       "lib/extractors.js",
       "lib/userscripts.js",
       "lib/content.js"
      ],
      "css": ["styles/general.css"],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "browser_action": {
     "default_icon" : "skin/fork19.png",
     "default_title": "taberareloo",
     "popup"        : "popup.html"
   },
   "options_page": "options.html",
   "icons": {
     "128": "skin/fork128.png",
     "64": "skin/fork64.png",
     "32": "skin/fork32.png",
     "16": "skin/fork16.png"
   },
   "minimum_chrome_version": "17.0",
   "default_locale": "en_US",
   "description": "Yet Another Tombloo on Chromium",
   "name": "Taberareloo",
   "permissions": [ "http://*/*", "https://*/*", "tabs", "bookmarks", "contextMenus", "cookies", "unlimited_storage", "notifications" ],
   "version": "2.0.60",
   "incognito": "split"
}
